{
  "version": 2,
  "buildCommand": "python -m pip install -r requirements.txt && python manage.py collectstatic --noinput",
  "devCommand": "python manage.py runserver 0.0.0.0:8000",
  "env": {
    "DJANGO_SETTINGS_MODULE": "coony.settings",
    "PYTHONUNBUFFERED": "1"
  },
  "builds": [
    {
      "src": "coony/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "coony/wsgi.py"
    }
  ]
}
